include "ctw_lib.bhs"

conquest
{

  labels {
    PLAYER,
    ENEMY,
  }
  
  new_world_rules();

  static int diff = get_difficulty();

  static int end_time = get_time_limit();
  int cur_time;
  
  run_once {
    new_world_setup();
  }

  cur_time = time_min();
  if (cur_time >= end_time) defeat(PLAYER);

}