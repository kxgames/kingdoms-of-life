			</ENTRY>
			<ENTRY name="RUSHRULES11">
				<STRING>{DIE ERSTEN 10 MINUTEN LANG FRIEDEN}: Spieler dürfen die ersten 10 Minuten lang niemanden angreifen.   </STRING>
			</ENTRY>
			<ENTRY name="RUSHRULES12">
				<STRING>{DIE ERSTEN 15 MINUTEN LANG FRIEDEN}: Spieler dürfen die ersten 15 Minuten lang niemanden angreifen.   </STRING>
			</ENTRY>
			<ENTRY name="RUSHRULES13">
				<STRING>{DIE ERSTEN 20 MINUTEN LANG FRIEDEN}: Spieler dürfen die ersten 20 Minuten lang niemanden angreifen.   </STRING>
			</ENTRY>
			<ENTRY name="RUSHRULES14">
				<STRING>{DIE ERSTEN 30 MINUTEN LANG FRIEDEN}: Spieler dürfen die ersten 30 Minuten lang niemanden angreifen.   </STRING>
			</ENTRY>
