			<STRING>Cliquez pour revivre une des grandes batailles de l'Histoire.</STRING>
		</ENTRY>
		<ENTRY name="skilltest">
			<STRING>Cliquez pour tester votre habileté à l'occasion d'une série de challenges spéciaux.</STRING>
		</ENTRY>
		<ENTRY name="main_load_scenario">
			<STRING>Cliquez pour démarrer un scénario. </STRING>
		</ENTRY>
		<ENTRY name="load_solo">
			<STRING>Cliquez pour charger une partie enregistrée.</STRING>
		</ENTRY>
		<ENTRY name="solo_recorded">
			<STRING>Cliquez pour lire une partie enregistrée.</STRING>
		</ENTRY>
		<ENTRY name="back">
			<STRING>Cliquez pour revenir au menu principal.</STRING>
		</ENTRY>
		<!--Multi Menu-->
		<ENTRY name="gamespy">
			<STRING>Cliquez pour démarrer une partie Internet via le service de correspondances en ligne GameSpy.</STRING>
		</ENTRY>
		<ENTRY name="lan">
			<STRING>Cliquez pour démarrer une partie sur votre réseau local ou sur Internet directe (TCP/IP).</STRING>
		</ENTRY>
		<ENTRY name="mp_recorded">
			<STRING>Cliquez pour lire une partie enregistrée.</STRING>
		</ENTRY>
		<ENTRY name="back">
			<STRING>Cliquez pour revenir au menu principal.</STRING>
		</ENTRY>
		<!--Options Menu-->
		<ENTRY name="options">
			<STRING>Cliquez pour définir vos préférences, remapper les raccourcis et gérer votre profil de joueur.</STRING>
		</ENTRY>
		<ENTRY name="editor">
			<STRING>Cliquez pour entrer dans l'éditeur de scénario qui vous permet de créer et de modifier des scénarios personnalisés.</STRING>
		</ENTRY>
		<ENTRY name="script_ide">
			<STRING>Cliquez pour exécuter l'éditeur de script, qui vous permet de créer, de modifier et de déboguer des scripts existants et personnalisés.</STRING>
		</ENTRY>
		<ENTRY name="unit_balance">
			<STRING>Cliquez pour exécuter l'outil d'équilibrage des unités, qui effectue des correspondances entre unités de types différents.</STRING>
