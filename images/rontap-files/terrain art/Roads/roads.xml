			<ROADFLAGS value="00010001"/>
			<!-- yellow line flags...starting from upper left, one bit per yellow line connect -->
			<YELLOWFLAGS value="00100001"/>
			<!-- white line flags -->
			<WHITEFLAGS value="00010010"/>
